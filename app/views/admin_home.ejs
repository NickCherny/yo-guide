<% layout('template.admin') %>

<h1>Добрый день <%=user%></h1>

<p><a href="/logout/admin">Выйти</a></p>

<form action="/admin/project/new" method="post" enctype="multipart/form-data" name="newProject">
  <input type="file" name="file" placeholder="Загрузить картинку">
  <input type="text" name="title" placeholder="Название проекта">
  <textarea name="description" id="description" cols="30" rows="10" placeholder="Описания проекта"></textarea>
  <input type="submit" value="Отправить">
</form>
